#include<stdio.h>
int main()
{
 int n,i,sum=0;
 printf("enter the number\n");
 scanf("%d",&n);
 for(i=1;i<=n/2;i++)
  {
   if(n%i==0)
   {
    sum=sum+i;
    }
    }
    if(sum==n)
     printf("the entered number is perfect number\n");
     else
     printf("the number is not a perfect number\n");
     return 0;
     }





